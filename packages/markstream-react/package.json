{
  "name": "markstream-react",
  "type": "module",
  "version": "0.0.3-beta.2",
  "description": "React renderer for stream-markdown based AST output used in markstream-vue.",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./index.css": "./dist/index.css",
    "./workers/katexRenderer.worker": "./dist/workers/katexRenderer.worker.js",
    "./workers/mermaidParser.worker": "./dist/workers/mermaidParser.worker.js"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite dev",
    "build": "vite build --mode npm",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit",
    "release": "bumpp && npm publish --access public"
  },
  "peerDependencies": {
    "katex": ">=0.16.22",
    "mermaid": ">=11",
    "react": ">=18",
    "react-dom": ">=18",
    "stream-markdown": ">=0.0.10",
    "stream-monaco": ">=0.0.7"
  },
  "peerDependenciesMeta": {
    "katex": {
      "optional": true
    },
    "mermaid": {
      "optional": true
    },
    "stream-markdown": {
      "optional": true
    },
    "stream-monaco": {
      "optional": true
    }
  },
  "dependencies": {
    "@floating-ui/dom": "^1.7.4",
    "clsx": "^2.1.1",
    "stream-markdown-parser": "workspace:*"
  },
  "devDependencies": {
    "@types/react": "^18.3.27",
    "@types/react-dom": "^18.3.7",
    "@vitejs/plugin-react-swc": "^3.11.0",
    "autoprefixer": "^10.4.23",
    "tailwindcss": "^3.4.19",
    "typescript": "^5.9.3",
    "vite": "^7.3.0"
  }
}
